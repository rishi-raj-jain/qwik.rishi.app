    "@edgio/core": "^6.1.4-next-1680160737-80f2d1c.0",
    "@edgio/devtools": "^6.1.4-next-1680160737-80f2d1c.0",
    "@edgio/prefetch": "^6.1.4-next-1680160737-80f2d1c.0",
