    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "autoprefixer": "^10.4.13",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "eslint": "8.36.0",
    "eslint-plugin-qwik": "0.24.0",
    "express": "4.18.2",
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.16",
    "prettier": "^2.8.6",
    "rehype-stringify": "^9.0.3",
